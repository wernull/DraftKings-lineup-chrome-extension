{
  "manifest_version": 2,

  "name": "DraftKings Lineup",
  "description": "This helps set lineups for DraftKings daily sports website",
  "version": "0.1",

  "background": {
      "persistent": false,
      "scripts": ["daily.js"]
  },
  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
   "activeTab"
  ],
  "web_accessible_resources": [
    "daily.css"
  ]
}
